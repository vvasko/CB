<div class="page-header">
  <h1>Please select your table
  </h1>
</div>

<div class="body_panel panel-default">
<div class="panel-body">
    <div class="row-eq-height">
    <% @tables.each do |table| %>
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 table_icon"><%=table.id %><%= button_to '', welcome_path(table.id), method: :get %></div>
    <% end %>
  </div>
</div>
</div>
